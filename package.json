{
  "name": "n8n-pulse",
  "type": "module",
  "version": "0.1.0",
  "description": "Community health dashboard and ecosystem explorer for n8n",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run update-all-history && astro build",
    "build:only": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "fetch-data": "tsx scripts/fetch-data.ts",
    "fetch-daily": "tsx scripts/fetch-daily.ts",
    "fetch-external": "tsx scripts/fetch-external.ts",
    "build-history": "tsx scripts/build-history.ts",
    "backfill-creators": "tsx scripts/backfill-creators-history.ts",
    "update-github-history": "tsx scripts/update-github-history.ts",
    "update-community-history": "tsx scripts/update-community-history.ts",
    "update-templates-history": "tsx scripts/update-templates-history.ts",
    "update-all-history": "npm run update-github-history && npm run update-community-history && npm run update-templates-history"
  },
  "keywords": [
    "n8n",
    "automation",
    "statistics",
    "community"
  ],
  "author": "Guido X Jansen",
  "license": "MIT",
  "dependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.16.7",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-chart-treemap": "^3.1.0",
    "date-fns": "^4.1.0",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
